    "dependencies": {
        "imagemin": "^8.0.1",
        "imagemin-webp": "^6.0.0",
        "onchange": "^7.1.0"
    },
    "devDependencies": {
        "autoprefixer": "*",
        "cssnano": "*",
        "npm-run-all": "*",
        "postcss": "*",
        "postcss-cli": "*",
        "postcss-scss": "*",
        "sass": "*"
    },
    "scripts": {
        "build-task:mycompile": "sass src/sass:css -s expanded",
        "build-task:prefix": "postcss css/**/*.css --base --use=autoprefixer --map=false --dir css",
        "build-task:min": "postcss css/**/*[!min].css --base --use=cssnano --map=false --dir css --ext .min.css",
        "dev": "onchange \"src/sass\" -- run-s build-task:*"
